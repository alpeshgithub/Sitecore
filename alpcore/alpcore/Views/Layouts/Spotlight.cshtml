@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
    <div class="highlight">
        @Html.Sitecore().Field("Image", Model.Item, new { mw = 300, mh = 400, hspace = 10, vspace = 10, @class = "test" })
        <p><strong>Make: </strong> @Html.Sitecore().Field("Make", Model.Item)</p>
        <p><strong>Model: </strong> @Html.Sitecore().Field("Model", Model.Item)</p>
        <p>@Html.Sitecore().Field("Website", Model.Item, new { text = "Read More about this car" })</p>
        <p>@Html.Sitecore().Field("Website", Model.Item, new { format = "dd/mm/yyyy" })</p>
    </div>
